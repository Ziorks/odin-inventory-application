<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="/style.css" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% categories.forEach(category => { %>
      <li>
        <a href="/categories/<%= category.id %>"><%= category.category %></a>
      </li>
      <% }) %>
    </ul>
    <a href="/categories/create">Add a new category</a>
  </body>
</html>
